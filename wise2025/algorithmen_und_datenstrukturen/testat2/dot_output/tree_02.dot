// Red Black Tree
digraph G {
	graph [ratio=.48];
	node [style=filled, color=black, shape=circle, width=.6
		fontname=Helvetica, fontweight=bold, fontcolor=white,
		fontsize=24, fixedsize=true];

	n0 [label="37"];
	n1 [label="15", fillcolor=red];
	nil0 [label="NIL", shape=record, width=.4, height=.25, fontsize=16];
	nil1 [label="NIL", shape=record, width=.4, height=.25, fontsize=16];
	nil2 [label="NIL", shape=record, width=.4, height=.25, fontsize=16];

	n1 -> nil0;
	n1 -> nil1;
	n0 -> n1;
	n0 -> nil2;
}
